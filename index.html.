# Tamato.github.io
background(255, 255, 255);

// tomato
noStroke();
fill(224, 90, 90);
ellipse(160, 200, 300, 300);
ellipse(220, 200, 300, 300);

// stem
fill(48, 130, 31);
rect(180, 21, 16, 70); 

// take a bite out of the tomato!
draw = function() {
    fill(255, 255, 255);
    ellipse(mouseX,mouseY, 60,60);
};
